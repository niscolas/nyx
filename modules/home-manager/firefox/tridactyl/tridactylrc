command reloadconfig source ~/.config/tridactyl/tridactylrc

bindurl web.whatsapp.com --mode=input <Escape> composite fillcmdline ; ex.hide_and_clear
bindurl web.whatsapp.com --mode=insert <Escape> composite fillcmdline ; ex.hide_and_clear
bindurl web.whatsapp.com --mode=normal <Enter> focusinput 2

bindurl music.youtube.com --mode=normal ;l hint -c #button-shape-like>button, #button-shape-dislike>button
bindurl chat.openai.com --mode-normal <Enter> focusinput

blacklistadd www.notion.so
blacklistadd gaming.amazon.com

